#include <stdio.h>

int main()
{
    int broj;
    int p0=0, p1=0, p2=0,p3=0,p4=0;
    int poz=-1, maxpoz=-1, max=-1;
    
    while(1)
    {
        if(scanf("%d", &broj))
        {
            for(poz=0,maxpoz=0,max=broj%10;broj>0;broj/=10, poz++)
            {
                if(broj%10>max)
                {
                    max=broj%10;
                    maxpoz=poz;
                }
            }
            switch(maxpoz)
            {
                case 0:
                p0++;
                break;
                case 1:
                p1++;
                break;
                case 2:
                p2++;
                break;
                case 3:
                p3++;
                break;
                case 4:
                p4++;
                break;
            }
        }
        else 
        break;
    }
    printf("0: %d\n1: %d\n2: %d\n3: %d\n4: %d\n", p0,p1,p2,p3,p4);
    return 0;
}
