#include <stdio.h>

int main()
{
    int n, n1;
    int zbirD1=0;
    scanf("%d", &n);
    
    for(int i=n-1;i>0;i--)
    {
        int zbirD=0;
        int n0=i;
        for(int j=1;j<i;j++)
        {
            if(i%j==0)
            {
                zbirD+=j;
            }
        }
        if(zbirD > zbirD1)
        {
            zbirD1 = zbirD;
            n1 = n0;
        }
        
    }
    printf("%d", n1 );
    
}
